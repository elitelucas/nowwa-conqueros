{
  "extends": "@tsconfig/node16/tsconfig.json",
  "lib": [
    "es5",
    "es6",
    "dom"
  ],
  "compilerOptions": {
    "rootDir": "src/",
    "outDir": "dist/",
    "jsx": "react-jsx",
    "lib": [
      "es5",
      "es6",
      "dom"
    ],
    "module": "CommonJS",
    "esModuleInterop": true,
    "target": "ES6",
    "skipLibCheck": true,
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "noFallthroughCasesInSwitch": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true
  },
  "include": [
    "src/Core/",
    "src/Models/",
    "src/Frontend/",
    "src/Compile.ts",
    "src/Utils/UTIL",
    "src/UTIL/Instances/TimerInstance.ts",
    "src/UTIL/CRYPT.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
